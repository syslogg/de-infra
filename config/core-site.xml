<?xml version="1.0"?>
<configuration>
  <!-- S3A Configuration -->
  <property>
    <n>fs.s3a.endpoint</n>
    <value>http://minio:9000</value>
  </property>
  
  <property>
    <n>fs.s3a.access.key</n>
    <value>admin</value>
  </property>
  
  <property>
    <n>fs.s3a.secret.key</n>
    <value>admin123</value>
  </property>
  
  <property>
    <n>fs.s3a.path.style.access</n>
    <value>true</value>
  </property>
  
  <property>
    <n>fs.s3a.connection.ssl.enabled</n>
    <value>false</value>
  </property>
  
  <property>
    <n>fs.s3a.impl</n>
    <value>org.apache.hadoop.fs.s3a.S3AFileSystem</value>
  </property>
  
  <!-- Default FS Configuration -->
  <property>
    <n>fs.defaultFS</n>
    <value>file:///</value>
  </property>
</configuration>